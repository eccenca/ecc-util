  },
  "devDependencies": {
    "@eccenca/dotfiles": "^2.4.0",
    "@eccenca/gulp-tasks": "^3.16.0",
    "should": "^13.2.1",
    "sinon": "^4.3.0"
  },
  "dependencies": {
    "lodash": "^4.17.5",
